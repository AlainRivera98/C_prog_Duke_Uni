#include <stdio.h>
#include <stdlib.h>

unsigned power(unsigned x, unsigned y);

void run_check(unsigned x, unsigned y, unsigned ans){
  if(power(x,y) != ans){

    printf("Case %u^%u = %u detected as wrong\n",x,y,ans);
    exit(EXIT_FAILURE);
  }
}

int main(void){
  run_check(2,3,8);
  //if(power(-1,2) == 1){return EXIT_FAILURE;}
  //if(power(0,0) != 1){return EXIT_FAILURE;}
  //if(power(2,3) != 8){return EXIT_FAILURE;}

  


  return EXIT_SUCCESS;
}
